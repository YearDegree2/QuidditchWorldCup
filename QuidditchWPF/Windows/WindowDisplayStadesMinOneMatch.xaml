<Window x:Class="QuidditchWPF.Windows.WindowDisplayStadesMinOneMatch"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Liste des stades avec au moins un match programmé" MinHeight="500" MinWidth="800"
        Closing="Window_Closing" Loaded="Window_Loaded">
    <StackPanel Orientation="Vertical">
        <ListBox Name="listStades" Loaded="listStades_Loaded"></ListBox>
    </StackPanel>
</Window>
